{
  "name": "cats-gg",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "9.6.1",
    "babel-core": "6.26.3",
    "babel-eslint": "7.2.3",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-preset-react-app": "^9.0.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "cat-facts": "^1.0.2",
    "chalk": "2.4.2",
    "css-loader": "0.28.11",
    "dotenv": "8.0.0",
    "dotenv-expand": "5.1.0",
    "eslint": "4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "2.2.1",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.14.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "2.0.0",
    "forever": "^1.0.0",
    "fs-extra": "8.1.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.8.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "promise": "8.0.3",
    "prop-types": "^15.7.2",
    "raf": "3.4.1",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-dev-utils": "^9.0.1",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.5",
    "reselect": "^4.0.0",
    "resolve": "1.11.1",
    "serve": "^11.1.0",
    "style-loader": "0.23.1",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "1.1.2",
    "webpack": "3.12.0",
    "webpack-dev-server": "3.7.2",
    "webpack-manifest-plugin": "2.0.4",
    "whatwg-fetch": "3.0.0"
  },
  "scripts": {
    "create-cool-cats-map": "node scripts/create-cool-cats-map.js",
    "download-cool-cats": "node scripts/download-cool-cats.js -f cool-cats.txt",
    "start": "node scripts/start.js",
    "serve": "PORT=3001 forever start scripts/start.js",
    "stop": "forever stop scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "transform-async-generator-functions"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "args": "^5.0.1",
    "babel-plugin-transform-async-generator-functions": "^6.24.1"
  }
}
